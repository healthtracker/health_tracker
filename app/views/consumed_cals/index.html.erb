<p id="notice"><%= notice %></p>

<h1>Listing Consumed Cals</h1>

<table>
  <thead>
    <tr>
      <th>Calories</th>
      <th>Calories on</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @consumed_cals.each do |consumed_cal| %>
      <tr>
        <td><%= consumed_cal.calories %></td>
        <td><%= consumed_cal.calories_on %></td>
        <td><%= link_to 'Show', consumed_cal %></td>
        <td><%= link_to 'Edit', edit_consumed_cal_path(consumed_cal) %></td>
        <td><%= link_to 'Destroy', consumed_cal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Consumed cal', new_consumed_cal_path %>
